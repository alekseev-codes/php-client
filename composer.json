{
  "name": "xcom-marketplace/client",
  "require": {
    "php": "^7.2.5 || ^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0",
    "guzzlehttp/psr7": "^2.4",
    "psr/http-message": "^1.0",
    "psr/http-client": "^1.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "XcomMarketplace\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "XcomMarketplace\\Client\\Tests\\": "tests/"
    }
  }
}
